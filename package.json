    "cross-env": "^5.0.1",
    "datatables.net": "^1.10.16",
    "datatables.net-bs4": "^1.10.16",
    "datatables.net-fixedheader": "^3.1.2",
    "datatables.net-fixedheader-bs4": "^3.1.3",
    "datatables.net-responsive": "^2.1.1",
    "datatables.net-responsive-bs4": "^2.2.0",
    "jquery": "^3.1.1",
    "laravel-mix": "^2.0.0",
    "lodash": "^4.17.4",
    "popper.js": "^1.12.3",
    "vue": "^2.1.10",
